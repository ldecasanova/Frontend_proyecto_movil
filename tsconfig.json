{
  "extends": "expo/tsconfig.base",
 "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react-native", // or "react" if you are using React for web
    "strict": true,
    "esModuleInterop": true, // Enable esModuleInterop
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
